{
  "name": "cidemo-frontend",
  "version": "0.4.0",
  "author": "Julie Ng <julie.ng@allianz.de>",
  "license": "MIT",
  "description": "AngularJS Frontend for continuous integration demo",
  "repository": {
    "type": "git",
    "url": "https://github.com/allianz-de/cidemo-frontend"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "serve:proxied": "ng serve --proxy-config ./config/proxy.conf.json",
    "serve:docker": "ng serve --host 0.0.0.0 --port 4200 --live-reload false --proxy-config ./config/proxy.conf.json",
    "build": "ng build",
    "build:prod": "ng build --aot --prod --progress false",
    "configure:nginx": "copyfiles --flat nginx.conf dist",
    "test": "ng test",
    "test:headless": "ng test --single-run --progress false --browser ChromeHeadless",
    "test:once": "ng test --single-run --progress false",
    "lint": "tslint --config ./config/tslint.json 'src/**/*.ts'",
    "lint:fix": "tslint --config ./config/tslint.json 'src/**/*.ts' --fix",
    "e2e": "ng e2e",
    "e2e:prod": "ng e2e --progress false --serve false --config ./config/protractor.prod.conf.js",
    "preflight:check": "npm run test:headless && npm run lint && npm run e2e",
    "release": "standard-version",
    "version:next": "semver \"$(node --print \"require('./package.json').version\")\" -i minor"
  },
  "private": true,
  "dependencies": {
    "@angular-redux/store": "^6.6.0",
    "@angular/animations": "^4.4.7",
    "@angular/common": "^4.4.7",
    "@angular/compiler": "^4.4.7",
    "@angular/core": "^4.4.7",
    "@angular/forms": "^4.4.7",
    "@angular/http": "^4.4.7",
    "@angular/platform-browser": "^4.4.7",
    "@angular/platform-browser-dynamic": "^4.4.7",
    "@angular/router": "^4.4.7",
    "core-js": "^2.5.7",
    "redux": "^3.7.2",
    "rxjs": "^5.5.11",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^4.4.7",
    "@angular/language-service": "^4.4.7",
    "@types/jasmine": "^2.5.54",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^6.0.113",
    "codelyzer": "~3.0.1",
    "copyfiles": "^1.2.0",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.3",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "semver": "^5.5.0",
    "standard-version": "^4.4.0",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
}
